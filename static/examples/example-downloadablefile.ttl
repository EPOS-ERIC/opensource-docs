#########
# Dataset Top Concept Scheme
#########
<category:facets/dataset-theme> a skos:Concept ;
       skos:definition  "Dataset" ;
       skos:prefLabel   "Dataset"
 .


#########
# Concept Scheme
#########

<category:payloadexample> a skos:ConceptScheme;
 skos:prefLabel "EXAMPLE1" ; 
 dct:title "EPOS Example" ;
 dct:description "Some payload example" ;
 foaf:logo "https://github.com/epos-eu/platform-gui-icons/blob/main/example.png?raw=true" ; 
 #foaf:homepage "" ;
 schema:color "#2e3486";
 schema:orderItemNumber "1";
 skos:hasTopConcept <category:facets/dataset-theme>;
.


#########
# Concepts
#########

<category:downloadblefileexample> a skos:Concept;
 skos:prefLabel "Downloadable file example"; #GUI
 skos:definition "This is an example";
 skos:inScheme <category:payloadexample>;
.


###############################################################################
### Organisations ###
###############################################################################

<PIC:000000000> a schema:Organization;
  schema:identifier [ a schema:PropertyValue;
    schema:propertyID "PIC";
    schema:value "000000000";
  ]; 
  schema:legalName "Organization test name";
  schema:address [ a schema:PostalAddress;
    #schema:streetAddress "";
    #schema:addressLocality "";
    #schema:postalCode "";
    schema:addressCountry "IT"; #use standard ISO 3166-1 (alpha-2 code)
  ]; 
  #schema:logo ""^^xsd:anyURI;
  schema:url "http://www.test.it"^^xsd:anyURI;
  #schema:email "";
. 


###############################################################################
### Dataset ###
###############################################################################

<https://example.org/dataset/downloadablefile/Dataset/001> a dcat:Dataset ;
        dct:identifier "https://example.org/dataset/downloadablefile/Dataset/001" ;
        dct:title "Downloadable file Dataset" ;
        dct:description "This is a sample downloadable file." ;
	    adms:identifier [ a adms:Identifier ;
    	    adms:schemeAgency "DOI" ;
    	    skos:notation "test/00000" ;
	    ];
        dct:type "http://purl.org/dc/dcmitype/Dataset"^^xsd:anyURI ;
        dct:accrualPeriodicity "http://purl.org/cld/freq/annual"^^xsd:anyURI ;
        dcat:theme <category:downloadblefileexample>;
        dcat:keyword "example","downloadable file"  ;
        dct:publisher <PIC:000000000>; 
        dcat:distribution <https://example.org/dataset/downloadablefile/Dataset/001/Distribution/001> ;
        dct:spatial [ a dct:Location ;
          locn:geometry "POLYGON((-26.0 72.0, 68.5 72.0, 68.5 23.5, -26.0 23.5, -26.0 72.0))"^^gsp:wktLiteral ;
        ];
        dct:temporal [ a dct:PeriodOfTime ;
                schema:startDate "1960-01-01T00:00:00Z"^^xsd:dateTime ;
                schema:endDate "2025-10-08T17:04:00Z"^^xsd:dateTime ;
        ] ;
        	dqv:hasQualityAnnotation [ a oa:Annotation ;
		oa:hasBody "https://test.eu"^^xsd:anyURI  ;
	];
.


###############################################################################
### Distribution ###
###############################################################################

<https://example.org/dataset/downloadablefile/Dataset/001/Distribution/001> a dcat:Distribution ;
        dct:identifier "https://example.org/dataset/downloadablefile/Dataset/001/Distribution/001" ;
        dct:title "Example Dataset Downloadable File" ;
        dct:description "This is a sample downloadable file." ;
        dct:issued "2025-10-08"^^xsd:date ;
        dct:modified "2025-10-08"^^xsd:date ;
        dct:type "http://publications.europa.eu/resource/authority/distribution-type/DOWNLOADABLE_FILE"^^xsd:anyURI ;
        dct:format "http://publications.europa.eu/resource/authority/file-type/RDF_TURTLE"^^xsd:anyURI ;
        dcat:downloadURL "https://raw.githubusercontent.com/EPOS-ERIC/opensource-docs/refs/heads/main/static/examples/example-downloadablefile.ttl"^^xsd:anyURI ;
        dcat:accessURL "https://raw.githubusercontent.com/EPOS-ERIC/opensource-docs/refs/heads/main/static/examples/example-downloadablefile.ttl"^^xsd:anyURI ;
        dct:license "https://creativecommons.org/licenses/by/4.0/"^^xsd:anyURI ;
.

