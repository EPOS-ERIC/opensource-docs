       skos:prefLabel   "Dataset"
 .



<category:geomagneticobservatis> a skos:ConceptScheme;
 skos:prefLabel "GEO" ; #GEOMAG
 dct:title "Geomagnetic Observations" ;
 dct:description "TCS Domain" ;
 foaf:logo "assets/img/logo/GEOMAG_logo.png" ;
 foaf:homepage "https://www.epos-eu.org/tcs/geomagnetic-observations" ;
 schema:color "#2e3486";
 schema:orderItemNumber "6";
 skos:hasTopConcept <category:facets/dataset-theme>;
.
<category:geomagneticdata> a skos:Concept;
 skos:prefLabel "Geomagnetic data";
 skos:definition "TCS Subdomain";
 skos:inScheme <category:geomagneticobservatis>;
 skos:notation "WP13-DDSS-001";
 skos:notation "WP13-DDSS-002";
 skos:notation "WP13-DDSS-003";
.



