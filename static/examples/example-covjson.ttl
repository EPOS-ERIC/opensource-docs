###############################################################################
### Dataset ###
###############################################################################

<https://example.org/dataset/CovJSONSample/Dataset> a dcat:Dataset;
    dct:identifier "https://example.org/dataset/CovJSONSample/Dataset";
    adms:identifier [ a adms:Identifier;
        adms:schemeAgency "DOI";
        skos:notation "test/00000";
    ];
    dct:title "Sample CovJSON Dataset";
    dct:description "Provides the sample dataset as a CovJSON payload via a web service endpoint.";
    dct:created "2025-10-07"^^xsd:date;
    dct:modified "2025-10-07"^^xsd:date;
    dct:issued "2025-10-07"^^xsd:date;
    dct:type "http://purl.org/dc/dcmitype/Collection"^^xsd:anyURI ;
    dct:accrualPeriodicity "http://purl.org/cld/freq/continuous"^^xsd:anyURI ;
    dcat:theme  <category:payloadexampleCovJSON> ;
    dcat:keyword "CovJSON", "example";
    dcat:contactPoint <http://orcid.org/0000-0000-0000-0011/ContactPoint> ;
    dct:publisher <ROR:000000>;
    dct:publisher <ROR:000002>;
    dcat:distribution <https://example.org/dataset/CovJSONSample/Distribution>;
    dct:temporal [ a dct:PeriodOfTime;
        schema:startDate "2025-01-01T00:00:00Z"^^xsd:dateTime;
        #schema:endDate "YYYY-MM-DDThh:mm:ssZ"^^xsd:dateTime;
    ];
    dct:spatial [ a dct:Location;
        locn:geometry "POLYGON((180.0 -90.0, -180.0 -90.0, -180.0 90.0, 180.0 90.0, 180.0 -90.0))"^^gsp:wktLiteral;
    ];	
	# This property refers to the quality assurance document URL
	dqv:hasQualityAnnotation [ a oa:Annotation ;
		oa:hasBody "https://example.pdf"^^xsd:anyURI  ;
	];	
.


###############################################################################
### Distribution ###
###############################################################################

<https://example.org/dataset/CovJSONSample/Distribution> a dcat:Distribution;
    dct:identifier "https://example.org/dataset/CovJSONSample/Distribution";
    dct:title "CovJSON Sample Dataset Distribution";
    dct:description "This sample CovJSON dataset is provided for demonstration and testing. It represents a geospatial coverage with a defined domain (sample locations) and range values illustrating attribute data.";
    dct:modified "2025-10-07"^^xsd:date;
    dct:issued "2025-10-07"^^xsd:date;
    dct:type "http://publications.europa.eu/resource/authority/distribution-type/WEB_SERVICE"^^xsd:anyURI;
    dct:conformsTo <https://example.org/dataset/CovJSONSample/Webservice>;
    dcat:accessURL <https://example.org/dataset/CovJSONSample/Operation> ;
    dct:license "https://creativecommons.org/licenses/by-nc/4.0/"^^xsd:anyURI ;
    dct:format "http://publications.europa.eu/resource/authority/file-type/JSON"^^xsd:anyURI ;
.


###############################################################################
### Web service ###
###############################################################################

<https://example.org/dataset/CovJSONSample/Webservice> a epos:WebService;
    schema:identifier "https://example.org/dataset/CovJSONSample/Webservice";
    schema:name "CovJSON Sample Web Service";
    schema:description "Web service providing access to the CovJSON sample dataset.";
    dcat:contactPoint <http://orcid.org/0000-0000-0000-0022/ContactPoint>;
    schema:datePublished "2025-10-07"^^xsd:date;
    schema:dateModified "2025-10-07"^^xsd:date;
    schema:provider <ROR:000003>;
    hydra:entrypoint "https://raw.githubusercontent.com/epos-eu/opensource-docs/refs/heads/test-CovJSON/static/example-payloads/epos-geo.json"^^xsd:anyURI ;
    schema:keywords "webservice", "test";
    hydra:supportedOperation <https://example.org/dataset/CovJSONSample/Operation> ;
    dct:license "https://creativecommons.org/licenses/by-nc/4.0/"^^xsd:anyURI;
    dct:conformsTo <https://example.org/dataset/CovJSONSample/APIDocumentation> ;
.


###############################################################################
###  Web service documentation ###
###############################################################################

<https://example.org/dataset/CovJSONSample/APIDocumentation> a hydra:ApiDocumentation ;
      hydra:title "CovJSON Sample Web Service Documentation" ;
      hydra:description "Documentation for using the CovJSON sample web service." ;
      hydra:entrypoint "https://example.org/docs/api/CovJSONSample"^^xsd:anyURI;
.


###############################################################################
### Web service operations ###
###############################################################################

<https://example.org/dataset/CovJSONSample/Operation> a hydra:Operation;
    hydra:method "GET"^^xsd:string;
    hydra:returns "covjson";
    hydra:property[ a hydra:IriTemplate;
        hydra:template "https://raw.githubusercontent.com/EPOS-ERIC/opensource-docs/refs/heads/main/static/examples/coverage.json"^^xsd:string;
    ] ;
.